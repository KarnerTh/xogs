defaultProfile: logfmt
profiles:
  - name: logfmt 
    parser: 
      logfmt: {}
    displayConfig:
      columns:
        - title: level
          width: 0.1
          valueKey: level
        - title: msg
          width: 0.8
          valueKey: msg
        - title: tag
          width: 0.1
          valueKey: tag
  - name: ping
    parser: 
      regex:
        values:
          - key: time
            regex: "time=(.*)"
          - key: ttl
            regex: 'ttl=(\d*)'
    displayConfig:
      columns:
        - title: time
          width: 0.1
          valueKey: time
        - title: ttl
          width: 0.1
          valueKey: ttl
        - title: msg
          width: 0.7
          valueKey: __raw
  - name: json
    parser: 
      json: {}
    displayConfig:
      columns:
        - title: level
          width: 0.1
          valueKey: level
        - title: env
          width: 0.1
          valueKey: env
        - title: nested
          width: 0.1
          valueKey: some.nested.data
        - title: msg
          width: 0.7
          valueKey: msg
