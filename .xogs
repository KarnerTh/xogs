defaultProfile: logfmt
profiles:
  - name: logfmt 
    parser: logfmt
    displayConfig:
      columns:
        - title: level
          width: 0.1
          valueKey: level
        - title: msg
          width: 0.8
          valueKey: msg
        - title: tag
          width: 0.1
          valueKey: tag
  - name: logfmt2
    parser: logfmt
    displayConfig:
      columns:
        - title: yeah
          width: 0.1
          valueKey: env
        - title: msg
          width: 0.7
          valueKey: msg
        - title: tag
          width: 0.1
          valueKey: tag
  - name: ping
    parser: ping
    displayConfig:
      columns:
        - title: time
          width: 0.1
          valueKey: time
        - title: msg
          width: 0.7
          valueKey: __raw
